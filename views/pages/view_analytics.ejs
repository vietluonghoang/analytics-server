<!DOCTYPE html>
<html>
<head>
</head>
<body>
<center>
<div class="container">
<h2>Analytics Results</h2>

<ul>
	<li><b>Number of users: </b>
		<%= eventCountByUsers.length%>
	</li>
	</ul>
	<br>
	<br>
	<table border="1" cellpadding="5">
		<% var order = 1 %>
		<tr><td></td><td align="center">Ads Id</td><td align="center">Times of appear</td><td align="center"></td></tr>
		<% eventCountByUsers.forEach(function(r) { %>
        	<tr><td><%= order%></td><td><%= r.adsid %> </td><td align="center"> <%= r.timesofaction %></td><td align="center"> <a href="https://wethoong-server.herokuapp.com/analytics/view/<%= r.adsid %>">View details</a></td></tr>
        	<% order += 1 %>
    	<% }); %>
	</table>
</div>
</center>
</body>
</html>
